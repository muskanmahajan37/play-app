{
  "name": "Play",
  "author": "Futurice",
  "description": "React Native App Starter Kit for Android and iOS",
  "license": "MIT",
  "homepage": "http://getpepperoni.com",
  "bugs": "https://github.com/futurice/pepperoni-app-kit/issues",
  "repository": {
    "type": "git",
    "url": "https://github.com/futurice/pepperoni-app-kit"
  },
  "keywords": [
    "React Native",
    "Starter Kit",
    "Bitrise",
    "Redux",
    "Auth0"
  ],
  "version": "0.3.0",
  "private": false,
  "scripts": {
    "start": "node node_modules/react-native/local-cli/cli.js start",
    "bundle:ios": "node ./node_modules/react-native/local-cli/cli.js bundle --platform ios --entry-file index.js --bundle-output ios/Play/main.jsbundle --dev=false --verbose",
    "test": "jest",
    "test:watch": "jest --watch",
    "lint": "eslint src test",
    "coverage": "rimraf coverage && jest --coverage",
    "release": "cd android && ./gradlew assembleRelease",
    "version": "support/version-ios.sh",
    "assets": "support/assets.sh"
  },
  "jest": {
    "preset": "react-native",
    "setupFiles": [
      "<rootDir>/test/setup.js"
    ],
    "transformIgnorePatterns": [
      "node_modules/(?!(jest-)?react-native|react-navigation)"
    ]
  },
  "dependencies": {
    "axios": "^0.16.2",
    "bluebird": "^3.3.5",
    "event-emitter": "^0.3.4",
    "immutable": "^3.7.6",
    "lodash": "^4.11.0",
    "lodash.isstring": "^4.0.1",
    "markdown-it": "^8.4.0",
    "moment": "^2.18.1",
    "prop-types": "^15.5.10",
    "react": "16.0.0-beta.5",
    "react-dom": "16.0.0-beta.5",
    "react-mixin": "^4.0.0",
    "react-native": "0.49.3",
    "react-native-ble-manager": "^6.2.3",
    "react-native-device-info": "^0.11.0",
    "react-native-elements": "^0.17.0",
    "react-native-fast-image": "^1.0.0",
    "react-native-fetch-blob": "^0.10.8",
    "react-native-google-analytics-bridge": "^5.3.3",
    "react-native-htmlview": "git+https://github.com/jsdf/react-native-htmlview.git#0.12.1",
    "react-native-pdf": "^1.3.5",
    "react-native-photo-view": "^1.5.2",
    "react-native-spinkit": "^1.1.1",
    "react-native-swiper": "^1.5.13",
    "react-native-vector-icons": "^4.4.0",
    "react-navigation": "^1.0.0-beta.12",
    "react-redux": "^5.0.1",
    "react-timer-mixin": "^0.13.3",
    "redux": "^3.6.0",
    "redux-logger": "^2.7.4",
    "redux-loop-symbol-ponyfill": "^2.2.0",
    "redux-promise": "^0.5.3",
    "redux-thunk": "^2.2.0",
    "standard-http-error": "^2.0.0"
  },
  "devDependencies": {
    "babel-core": "^6.9.0",
    "babel-eslint": "^7.1.0",
    "babel-jest": "^17.0.2",
    "babel-polyfill": "^6.9.0",
    "babel-preset-react-native": "^1.9.0",
    "babel-preset-stage-0": "^6.5.0",
    "babel-register": "^6.9.0",
    "enzyme": "^3.1.0",
    "eslint": "^3.10.1",
    "eslint-plugin-babel": "^3.2.0",
    "eslint-plugin-react": "^6.7.1",
    "fetch-mock": "^5.5.0",
    "istanbul": "1.0.0-alpha.2",
    "jest": "^17.0.2",
    "plop": "^1.7.4",
    "react-addons-test-utils": "~16.0.0-alpha.3",
    "react-native-mock": "~0.2.5",
    "react-test-renderer": "~16.0.0-alpha.6",
    "remote-redux-devtools": "^0.5.7",
    "rimraf": "^2.5.2"
  }
}
